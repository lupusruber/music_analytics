docker compose up -d

docker run -d \
  --network host \
  events:1.0 \
    -c "examples/example-config.json" \
    --start-time "2021-01-01T00:00:00" \
    --end-time "2024-12-30T23:00:00" \
    --nusers 2000 \
    --kafkaBrokerList localhost:9092


  docker run -d \
  --network kafka_kafka-net \
  events:1.0 \
    -c "examples/example-config.json" \
    --start-time "2021-01-01T00:00:00" \
    --end-time "2021-12-01T00:00:00" \
    --nusers 100 \
    --kafkaBrokerList kafka:9092

